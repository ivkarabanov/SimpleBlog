
@model IList<JustBlog.Core.Objects.Category>

<div>
    <h3>Категории</h3>

    @if (Model.Count > 0)
    {
        <ul>
            @foreach (var category in Model)
            {
                <li>@Html.CategoryLink(category)</li>
            }
        </ul>
    }
    else
    {
        <p>Не найдено ни одной категории!</p>
    }


</div>